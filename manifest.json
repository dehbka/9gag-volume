{
  "manifest_version": 3,
  "name": "9gag Volume & Sound control",
  "version": "0.4",
  "description": "This plugin adds volume and time control to video files on 9gag.",
  "author": "dehbkaa@gmail.com",
  "icons": {
    "16": "/icons/16.png",
    "48": "/icons/48.png",
    "128": "/icons/128.png"
  },
  "action": {
    "default_icon": "/icons/128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://9gag.com/*"
      ],
      "js": ["main.js"],
      "css": ["main.css"]
    }
  ]
}
